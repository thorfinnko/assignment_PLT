{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:import/typescript",
        "airbnb",
        "airbnb/hooks",
        "plugin:react/recommended", 
        "prettier",
        "prettier/react"
    ],
    "parser": "@typescript-eslint/parser", 
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react", 
        "react-native"
    ],
    "rules": {
    }
}
